    <Parameter name="dbname">osm2pgsql</Parameter>
    <Parameter name="table">
    (select simplified_way as way,name,ref from planet_osm_polygon where boundary='political' AND hstore(tags)-&gt;'political_division' = 'circonscription_l&#233;gislative') as cantons</Parameter>
    <Parameter name="estimate_extent">false</Parameter>
    <Parameter name="extent">-20037508,-19929239,20037508,19929239</Parameter>
  </Datasource>
