<!DOCTYPE Map>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +no_defs +over" background-color="transparent" minimum-version="0.7.2">
<Style name="admin_simplified">
	<Rule>
		<MinScaleDenominator>1000000</MinScaleDenominator>
		<PolygonSymbolizer fill-opacity="0.3" fill="#00FF00"/><LineSymbolizer stroke="#00FF00" stroke-width="1" stroke-opacity="0.8"/></Rule>
<Rule>
	
	<MaxScaleDenominator>6000000</MaxScaleDenominator>
	<MinScaleDenominator>1000000</MinScaleDenominator>
	<TextSymbolizer size="9" fill="black" face-name="DejaVu Sans Bold" halo-radius="1" wrap-width="10">[name]</TextSymbolizer>
	<TextSymbolizer dy="-10" size="9" fill="black" face-name="DejaVu Sans Bold" halo-radius="1" wrap-width="10">[ref]</TextSymbolizer>
</Rule>
</Style>
<Style name="admin">
	<Rule>
	<MaxScaleDenominator>1000000</MaxScaleDenominator>
	<PolygonSymbolizer fill-opacity="0.3" fill="#00FF00"/><LineSymbolizer stroke="#00FF00" stroke-width="1" stroke-opacity="0.8"/></Rule>
	<Rule>
		<MaxScaleDenominator>1000000</MaxScaleDenominator>
		<TextSymbolizer size="9" fill="black" face-name="DejaVu Sans Bold" halo-radius="1" wrap-width="10">[name]</TextSymbolizer>
		<TextSymbolizer dy="-10" size="9" fill="black" face-name="DejaVu Sans Bold" halo-radius="1" wrap-width="10">[ref]</TextSymbolizer>
	</Rule>
</Style>

  <Layer name="admin" status="on" srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +no_defs +over">
    <StyleName>admin</StyleName>
    <Datasource>
      <Parameter name="type">postgis</Parameter>
	<Parameter name="host">osm2pgsql-monde.openstreetmap.fr</Parameter>
      <Parameter name="user">layers</Parameter>
      <Parameter name="password">ootaephi</Parameter>
      <Parameter name="dbname">osm</Parameter>
      <Parameter name="table">
      (select way,admin_level,name,ref from planet_osm_polygon where boundary='administrative' and admin_level='4' ) as admin</Parameter>
      <Parameter name="estimate_extent">false</Parameter>
      <Parameter name="extent">-20037508,-19929239,20037508,19929239</Parameter>
    </Datasource>
  </Layer>

  <Layer name="admin_simplified" status="on" srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0 +k=1.0 +units=m +nadgrids=@null +no_defs +over">
	  <StyleName>admin_simplified</StyleName>
	  <Datasource>
		  <Parameter name="type">postgis</Parameter>
	<Parameter name="host">osm2pgsql-monde.openstreetmap.fr</Parameter>
		  <Parameter name="user">layers</Parameter>
		  <Parameter name="password">ootaephi</Parameter>
		  <Parameter name="dbname">osm</Parameter>
		  <Parameter name="table">
			  (select way as way,name,ref from planet_osm_polygon where boundary='administrative' and admin_level='4' ) as admin</Parameter>
		  <Parameter name="estimate_extent">false</Parameter>
		  <Parameter name="extent">-20037508,-19929239,20037508,19929239</Parameter>
	  </Datasource>
  </Layer>
</Map>